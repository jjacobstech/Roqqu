{
    "name": "jjacobstech/roqqu",
    "description": "This is Roqqu's SDK for Laravel",
    "type": "library",
    "require": {
        "php":"^8.1",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/http": "^12.20"
    },
    "autoload": {
        "psr-4": {
            "Jjacobstech\\Roqqu\\": "src/"
        },
        "files": [
            "src/Roqqu.php"
        ]
    },
    "authors": [
        {
            "name": "Jacobs Joshua",
            "email": "jacobsjoshua81@gmail.com"
        }
    ],

    "minimum-stability": "stable",
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
