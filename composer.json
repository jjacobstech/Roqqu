{
  "name": "jjacobstech/roqqu",
  "description": "This is Roqqu's SDK for Laravel",
  "type": "library",
  "keywords": [
    "roqqu",
    "sdk",
    "laravel",
    "api",
    "cryptocurrency"
  ],
  "readme": "/README.md",
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.9",
    "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0"
  },
  "provide": {
    "illuminate/support": "*"
  },
  "autoload": {
    "psr-4": {
      "Jjacobstech\\Roqqu\\": "src/"
    },
    "files": [
      "src/RoqquApi.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jjacobstech\\Roqqu\\RoqquServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer dump-autoload -o"
    ]
  },
  "authors": [
    {
      "name": "Jacobs Joshua",
      "email": "jacobsjoshua81@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require-dev": {
    "pestphp/pest": "^3.8"
  },
  "version": "1.0.0",
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
